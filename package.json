{
  "name": "demo",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/web-vitals.umd.cjs",
  "module": "dist/web-vitals.js",
  "unpkg": "dist/web-vitals.iife.js",
  "scripts": {
    "build:ts": "tsc -b",
    "build:js": "rollup -c",
    "build": "run-s clean build:ts build:js",
    "dev": "run-p watch",
    "watch": "run-p watch:*",
    "watch:ts": "tsc -b -w",
    "watch:js": "rollup -c -w",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "version": "run-s build",
    "format": "prettier \"**/*.{cjs,css,html,js,json,md,ts,yml,yaml}\" --write --ignore-path .gitignore",
    "format:check": "prettier \"**/*.{cjs,css,html,js,json,html,md,ts,yml,yaml}\" --check --ignore-path .gitignore",
    "lint": "eslint \"*.js\" \"src/*.ts\" ",
    "lint:fix": "eslint --fix \"*.js\" \"src/*.ts\" ",
    "postversion": "git push --follow-tags",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "eslint": "8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "typescript": "5.3.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "quoteProps": "preserve",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "husky": "8.0.3"
  }
}
